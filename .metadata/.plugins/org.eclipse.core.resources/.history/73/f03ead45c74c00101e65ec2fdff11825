package summerpep2.Interfaces;

public class Client {
    public static void main(String[] args) {
        phone myPhone = new phone();
        System.out.println("PHONE");
        myPhone.PlayMedia();
        myPhone.MakeCalls();
        myPhone.ConnectToWifi();
        
        SmartTv s=new SmartTv();
        System.out.println("SMART TV");
        s.ConnectToWifi();
        s.PlayMedia();
    }
}
